{
  "app": {
    "name": "Repo Default"
  },
  "server": {
    "host": "127.0.0.1",
    "port": 8000
  },
  "datastore": "@dadi/api-mongodb",
  "auth": {
    "tokenUrl": "/token",
    "tokenTtl": 1800000,
    "clientCollection": "client-store",
    "tokenCollection": "token-store",
    "datastore": "@dadi/api-mongodb",
    "database": "testdb"
  },
  "caching": {
    "ttl": 300,
    "directory": {
      "enabled": false,
      "path": "./cache/api",
      "extension": "json"
    },
    "redis": {
      "enabled": false,
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "paths": {
    "collections": "test/acceptance/workspace/collections",
    "endpoints": "test/acceptance/workspace/endpoints"
  },
  "logging": {
    "enabled": true,
    "level": "info",
    "path": "./log",
    "filename": "api",
    "extension": "log"
  },
  "feedback": false,
  "cors": false,
  "cluster": false
}
